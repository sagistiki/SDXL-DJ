{
  "name": "sdxl-dj",
  "version": "0.1.0",
  "description": "Real-time AI visual DJ app with SDXL-Turbo",
  "main": "electron/main.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:vite\" \"wait-on http://localhost:5173 && npm run dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "electron .",
    "build": "vite build && electron-builder",
    "preview": "vite preview",
    "download-model": "node scripts/download-model.js",
    "make": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "electron": "^28.1.0",
    "electron-builder": "^24.9.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
    "@xenova/transformers": "^2.17.2",
    "lucide-react": "^0.303.0",
    "onnxruntime-node": "^1.22.0-rev",
    "onnxruntime-web": "^1.16.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "systeminformation": "^5.27.6",
    "webmidi": "^3.1.8"
  },
  "build": {
    "appId": "com.sdxldj.app",
    "productName": "SDXL DJ",
    "directories": {
      "output": "dist"
    },
    "files": [
      "electron/**/*",
      "dist/**/*",
      "public/models/**/*"
    ],
    "mac": {
      "category": "public.app-category.music"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
